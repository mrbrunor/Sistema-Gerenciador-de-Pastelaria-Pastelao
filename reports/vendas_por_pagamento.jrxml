	<parameter name="TIPO_PGTO" class="java.lang.String"/>
	<parameter name="NOME_PGTO" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
    SISTEMAPASTELAO.PRODUTO.NUMPROD AS "ID do Produto",
    SISTEMAPASTELAO.PRODUTO.DESCPROD AS "Nome do Produto",
    SISTEMAPASTELAO.PRODUTO.VALORPROD AS "Valor Unitario",
    SUM(SISTEMAPASTELAO.ITEMPEDIDO.QTDPROD) AS "Quantidade",
    SUM(SISTEMAPASTELAO.ITEMPEDIDO.TOTPROD) AS "Valor Total",
    SISTEMAPASTELAO.PEDIDO.DATAPEDIDO AS "Data"
FROM
    SISTEMAPASTELAO.PRODUTO
        INNER JOIN
    SISTEMAPASTELAO.ITEMPEDIDO ON SISTEMAPASTELAO.PRODUTO.IDPROD = SISTEMAPASTELAO.ITEMPEDIDO.IDPROD
        INNER JOIN
    SISTEMAPASTELAO.PEDIDO ON SISTEMAPASTELAO.ITEMPEDIDO.IDPEDIDO = SISTEMAPASTELAO.PEDIDO.IDPEDIDO
        INNER JOIN
    SISTEMAPASTELAO.FORMAPAGAMENTO ON SISTEMAPASTELAO.PEDIDO.IDFORMAPGTO = SISTEMAPASTELAO.FORMAPAGAMENTO.IDFORMAPGTO
WHERE
    SISTEMAPASTELAO.PEDIDO.DATAPEDIDO BETWEEN $P{DATA_INI} AND $P{DATA_FIM}
        AND SISTEMAPASTELAO.PEDIDO.ESTADOPEDIDO = 'Finalizado'
        AND SISTEMAPASTELAO.FORMAPAGAMENTO.TIPOFORMAPGTO LIKE $P{TIPO_PGTO}
        AND SISTEMAPASTELAO.FORMAPAGAMENTO.NOMEFORMAPGTO LIKE $P{NOME_PGTO}
GROUP BY SISTEMAPASTELAO.PEDIDO.DATAPEDIDO, 
        SISTEMAPASTELAO.ITEMPEDIDO.IDPROD
ORDER BY SISTEMAPASTELAO.PEDIDO.DATAPEDIDO ASC,
        SISTEMAPASTELAO.PRODUTO.NUMPROD]]>
	</queryString>
